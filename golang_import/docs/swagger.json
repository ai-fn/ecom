{
    "swagger": "2.0",
    "info": {
        "description": "API for product imoprt",
        "title": "Products Import API",
        "contact": {},
        "version": "1.12.2"
    },
    "paths": {
        "/api/upload/{filename}": {
            "put": {
                "description": "Загрузка файла с товарами для импорта",
                "consumes": [
                    "multipart/form-data"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Import"
                ],
                "summary": "Загрузка файла товаров",
                "operationId": "uploadFile",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Имя файла",
                        "name": "filename",
                        "in": "path",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "Тип данных для импорта (PRODUCTS, BRANDS)",
                        "name": "type",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "default": "Bearer \u003cAdd access token here\u003e",
                        "description": "Insert your access token",
                        "name": "Authorization",
                        "in": "header",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    }
}